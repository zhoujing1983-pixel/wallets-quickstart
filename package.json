{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "wallets-quickstart",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -n web,worker,agent,ngrok -c blue,magenta,cyan,green \"next dev --turbopack\" \"npm run email:worker\" \"npm run agent:openai\" \"npm run ngrok\"",
    "dev:all": "npm run docker:up && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "colima:start": "colima start --disk 40",
    "colima:stop": "colima stop",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "redis:start": "brew services start redis",
    "dev:init": "npm run colima:start && npm run docker:up",
    "dev-shutdown": "npm run docker:down && npm run colima:stop",
    "agent:openai": "tsx scripts/voltagentopenai.ts",
    "agent:openai:local": "tsx scripts/voltagentopenai.ts --local",
    "treasure:wallet": "tsx scripts/treasureWallet.ts",
    "email:worker": "tsx scripts/email-worker.ts",
    "ngrok": "ngrok http 3000"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.5",
    "@ai-sdk/openai": "^3.0.5",
    "@ai-sdk/provider-utils": "^4.0.4",
    "@crossmint/client-sdk-react-ui": "2.6.6",
    "@vercel/analytics": "^1.5.0",
    "@voltagent/core": "^2.0.6",
    "@voltagent/libsql": "^2.0.2",
    "@voltagent/server-hono": "^2.0.2",
    "ai": "^6.0.15",
    "better-sqlite3": "^11.7.0",
    "bullmq": "^5.66.4",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "gpt-tokenizer": "^3.4.0",
    "ioredis": "^5.8.2",
    "mammoth": "^1.11.0",
    "next": "^16.1.0",
    "nodemailer": "^6.10.1",
    "ollama-ai-provider-v2": "^1.5.5",
    "pdf-parse": "^2.4.5",
    "pg": "^8.12.0",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-transition-group": "^4.4.5",
    "redis": "^5.10.0",
    "sqlite-vec": "^0.1.7-alpha.2",
    "sqlite-vec-darwin-arm64": "^0.1.7-alpha.2",
    "tailwind-merge": "^3.0.2",
    "tw-animate-css": "^1.2.4",
    "xlsx": "^0.18.5",
    "zod": "^4.2.1"
  },
  "optionalDependencies": {
    "sqlite-vec-darwin-arm64": "0.1.7-alpha.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.19.27",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-transition-group": "^4.4.12",
    "@voltagent/cli": "^0.1.20",
    "tailwindcss": "^4",
    "tsdown": "^0.18.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
